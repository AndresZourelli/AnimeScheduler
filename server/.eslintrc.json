{
    "extends": ["airbnb","prettier"],
    "rules":{
        
        "no-underscore-dangle": "off",
        "react/self-closing-comp": [
            "error",
            {
              "component": true,
              "html": true
            }
          ]
    },
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2021
    }
}